{
  "tab_bar": {
    "show": true
  },
  "features": {
    "edit_prediction_provider": "zed"
  },
  "languages": {
    "Go": {
      "show_edit_predictions": true
    }
  },
  "agent": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "claude-3.7-sonnet-thought"
    }
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "base_keymap": "Cursor",
  "theme": "Carbonfox - blurred",
  "vim_mode": true,
  "ui_font_size": 15,
  "buffer_font_size": 17,
  "ui_font_family": "FiraCode Nerd Font Mono",
  "buffer_font_family": "FiraCode Nerd Font Mono",
  "terminal": {
    "font_size": 15
  },
  "relative_line_numbers": true,
  "vim": {
    "use_system_clipboard": "always"
  },
  "gutter": {
    "folds": true
  },
  "indent_guides": {
    "coloring": "indent_aware"
  },
  "context_servers": {
    "postgres-context-server": {
      "source": "extension",
      "settings": {
        "database_url": "postgresql://postgres:postgres@localhost:5432/vulnsrv?sslmode=disable"
      }
    }
  }
}
