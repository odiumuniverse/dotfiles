{
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "base_keymap": "SublimeText",
  "theme": "Github Light",
  "vim_mode": true,
  "ui_font_size": 15,
  "buffer_font_size": 16,
  "ui_font_family": "RobotoMono Nerd Font",
  "buffer_font_family": "RobotoMono Nerd Font",
  "terminal": { "font_size": 15 },
  "relative_line_numbers": true,
  "vim": { "use_system_clipboard": "always" },
  "gutter": {
    "folds": true
  },
  "indent_guides": {
    "coloring": "indent_aware"
  }
}
